@techreport{janabiImportanciaMedirRiesgo2021,
  title = {La Importancia de Medir El Riesgo de Liquidez Con Aplicaciones Inteligentes},
  author = {Janabi, Al and M, Mazin A.},
  year = {2021},
  month = may,
  address = {{Rochester, NY}},
  institution = {{Social Science Research Network}},
  abstract = {En los \'ultimos d\'ias, se han encendido todas las alarmas ante el riesgo de recesi\'on de algunas de las principales econom\'ias del mundo (Alemania, Reino Unido, Italia, Brasil y M\'exico). La desaceleraci\'on afecta a varias regiones del mundo y puede llegar generalizarse, agravando la desconfianza de los inversores y la inestabilidad de los mercados financieros provocada por la guerra comercial entre EE. UU. y China, entre otros factores de incertidumbre. Los inversores han trasladado parte de sus carteras de inversi\'on a activos considerados refugio, como los bonos soberanos; la desconfianza de los inversores se encuentra en m\'inimos hist\'oricos de los \'ultimos a\~nos. Gestionar tu cartera de inversi\'on cuando se avecina un final de ciclo econ\'omico puede ser peliagudo. Si bien hay expertos que recomiendan reequilibrar la distribuci\'on de tus activos, comprar bonos del tesoro, enfocarte en materias primas, productos b\'asicos o inversi\'on inmobiliaria, lo realmente importante es aprender a gestionar el riesgo.  En el actual contexto de incertidumbre y condiciones de mercado adversas, evaluar la optimizaci\'on del desempe\~no de las carteras de inversi\'on, as\'i como el riesgo de liquidez, es esencial, como explico en el cap\'itulo ``Theoretical and practical foundations of liquidity-adjusted value-at-risk (LVaR): optimization algorithms for portfolio selection and management'', el cual est\'a incluido en el libro Expert Systems in Finance. Smart Financial Applications in Big Data Environments (Routledge, Taylor \& Francis Group, 2019), publicado recientemente.},
  file = {C\:\\Users\\Jhon\\Zotero\\storage\\7GPXFXSI\\papers.html},
  keywords = {Emerging Markets,Liquidity Risk,Liquidity-Adjusted Value at Risk,Machine Learning,Portfolio Management,Risk Management},
  language = {en},
  number = {ID 3840336},
  type = {{{SSRN Scholarly Paper}}}
}

@article{jaramilloRiesgoLiquidezSu2011,
  title = {{Riesgo de liquidez y su incidencia en la rentabilidad de las oficinas operativas de la cooperativa de ahorro y cr\'edito OSCUS Cia Ltda en la provincia de Tungurahua en el a\~no 2010}},
  author = {Jaramillo, Cruz and Betzab{\'e}, Mayra},
  year = {2011},
  abstract = {En esta investigaci\'on se realiz\'o una an\'alisis exhaustivo sobre el riesgo de liquidez, cu\'ales podr\'ian ser los factores de riesgo que expondr\'ian a la entidad, pues bien este an\'alisis se lo hizo por cada oficina operativa de la Cooperativa Oscus en la Provincia de Tungurahua las mismas que est\'an ubicadas en Ambato, Ba\~nos, Pelileo, Patate y P\'illaro debido a que estas oficinas son totalmente vulnerables ya que todo el control de riesgos se lo hace de una manera consolidada, dejando la incertidumbre de qu\'e sucede con cada oficina operativay c\'omo se maneja el riesgo dentro de las mismas.  Una vez comprobado que hace falta un mayor control de una manera individual a trav\'es de la conclusiones presentadas, se propuso la aplicaci\'on de la Metodolog\'ia VAR (Valor en Riesgo) para un control y monitoreo del riesgo de liquidez por cada una de ellas, este m\'etodo persigue dar un valor determinado estableciendo l\'imites en las fluctuaciones de dinero a trav\'es de la cuenta 210135 Dep\'ositos de Ahorro. De esta manera determinamos cu\'ales han sido las Oficinas Operativas m\'as vulnerables y con cuyos resultados generar los an\'alisis correspondientes para la posterior toma de decisiones.  A m\'as de esto se pudo concluir que las diferentes variaciones en esta cuenta pueden afectar a la rentabilidad con respecto, a cuanto dejan de ganar en colocaci\'on de carteras de cr\'edito y de inversi\'on por esta falta de control en el riesgo de liquidez.},
  annotation = {Accepted: 2012-05-16T13:42:07Z},
  copyright = {openAccess},
  language = {spa}
}

@article{johnsonValueRiskAjustado2000,
  title = {Value at {{Risk Ajustado}} Por {{Liquidez}}: {{Una Aplicaci\'on}} a Los {{Bonos Soberanos Chilenos}}},
  shorttitle = {Value at {{Risk Ajustado}} Por {{Liquidez}}},
  author = {Johnson, Christian},
  year = {2000},
  month = jan,
  abstract = {The aim of this article is 2-fold: first to test the adequacy of Pareto distributions to describe the tail of financial returns in emerging and developed markets, and second to study the possible correlation between stock market indices observed returns and return's extreme distributional characteristics measured by Value at Risk and Expected Shortfall. We test the empirical model using daily data from 41 countries, in the period from 1995 to 2005. The findings support the adequacy of Pareto distributions and the use of a log linear regression estimation of their parameters, as an alternative for the usually employed Hill's estimator. We also report a significant relationship between extreme distributional characteristics and observed returns, especially for developed countries.},
  journal = {Central Bank of Chile, Working Papers Central Bank of Chile}
}

